    "js = json.loads(rawdata)\n",
    "\n",
    "numbs = js[\"comments\"]\n",
    "\n",
    "ints =  [i[\"count\"] for i in numbs]\n",
    "\n",
    "print 'the sum is:', sum(ints)\n",
    "\n"
   ]
