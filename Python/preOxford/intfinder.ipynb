    "import re\n",
    "import itertools\n",
    "\n",
    "filer = open('regexptest.txt')\n",
    "\n",
    "intlist = []\n",
    "\n",
    "for line in filer:\n",
    "    y = re.findall('[0-9]+', line)\n",
    "    intlist.append(y)\n",
    "   \n",
    "    \n",
    "numbs = list(itertools.chain.from_iterable(intlist))\n",
    "\n",
    "inter = [int(x) for x in numbs]\n",
    "\n",
    "summed = sum(inter)\n",
    "\n",
    "print summed\n",
    "\n",
    "\n",
    "\n"
   ]
